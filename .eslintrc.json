{
  "extends": "next/core-web-vitals",
  "rules": {
      "@next/next/no-img-element": "off",
      "eqeqeq": "warn",
      "react/prop-types": "off",
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "double"
      ],
      "semi": [
          "error",
          "never"
      ]
  }
}
